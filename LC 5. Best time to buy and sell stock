class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int minimumprices = prices[0];
        int maxprofi = 0;
        for(int i=1;i<prices.size();i++){
            maxprofi = max(maxprofi,prices[i]-minimumprices);
            minimumprices = min(prices[i],minimumprices);
        }
        return maxprofi;
    }
};
