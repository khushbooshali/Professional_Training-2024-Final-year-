using namespace std;
int factorial(int num)
{
    int fact = 1;
    for(int i = num; i >= 1; i--)
        fact *= i;
    return fact;
}
int main()
{
    int n,r;  
    cin>>n>>r;  
    int p = factorial(r) / factorial(r - n);
    cout <<  p << endl;
    return 0;
}

        
 
