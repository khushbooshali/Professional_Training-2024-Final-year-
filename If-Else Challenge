using namespace std;
void checkWeirdness(int n) {
    if (n % 2 != 0) {
        cout << "Weird" << endl;
    } else {
        if (n >= 2 && n <= 5) {
            cout << "Not Weird" << endl;
        } else if (n >= 6 && n <= 20) {
            cout << "Weird" << endl;
        } else if (n > 20) {
            cout << "Not Weird" << endl;
        }
    }
}

int main() {
    int n;
    cin >> n;
    checkWeirdness(n);
    return 0;
}
