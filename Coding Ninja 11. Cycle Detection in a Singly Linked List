#include<unordered_set>
using namespace std;
bool detectCycle(Node *head)
{

    unordered_set<Node*> obj;
    Node *temp = head;
    while(temp!=NULL){
        if(obj.find(temp)!= obj.end())
            return true;
        else{
            obj.insert(temp);
            temp = temp->next;

        }

    }
    return false;
}
